{
    "name": "survos/media-bundle",
    "description": "Manage media without direct relationships",
    "keywords": [
        "symfony-ux",
        "symfony"
    ],
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^8.4",
        "symfony/config": "^7.3||^8.0",
        "symfony/dependency-injection": "^7.3||^8.0",
        "symfony/http-kernel": "^7.3||^8.0",
        "twig/twig": "^3.4",
        "doctrine/dbal": "^4.3.2",
        "doctrine/doctrine-bundle": "^2.7|^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "symfony/browser-kit": "^7.3||^8.0",
        "symfony/framework-bundle": "^7.3||^8.0",
        "symfony/phpunit-bridge": "^7.3||^8.0",
        "symfony/twig-bundle": "^7.3||^8.0",
        "symfony/var-dumper": "^7.3||^8.0",
        "openai-php/client": "^0.16.1",
        "symfony/http-client": "^7.3||^8.0",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Survos\\MediaBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
